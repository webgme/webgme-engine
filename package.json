{
  "name": "webgme-engine",
  "version": "2.19.7",
  "description": "WebGME server and Client API without a GUI",
  "main": "index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "start": "node ./src/bin/start_server.js",
    "prepublishOnly": "node ./utils/prepublish.js",
    "postinstall": "node ./utils/postinstall.js",
    "test": "node ./node_modules/mocha/bin/mocha --recursive test",
    "test_ci": "node ./node_modules/mocha/bin/mocha -R dot --timeout 10000 --recursive test",
    "test_cover": "node ./node_modules/istanbul/lib/cli.js --hook-run-in-context cover node_modules/mocha/bin/_mocha -- -R spec --timeout 10000 --recursive test",
    "test_browser": "node ./node_modules/karma/bin/karma start karma.conf.js --browsers Firefox --single-run --no-auto-watch",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc_conf.json",
    "apply": "node ./src/bin/apply.js",
    "diff": "node ./src/bin/diff.js",
    "export": "node ./src/bin/export.js",
    "import": "node ./src/bin/import.js",
    "merge": "node ./src/bin/merge.js",
    "plugin": "node ./src/bin/run_plugin.js",
    "pluginHook": "node ./src/bin/plugin_hook.js",
    "users": "node ./src/bin/usermanager.js",
    "clean_up": "node ./src/bin/clean_up.js",
    "stats": "node ./src/bin/storage_stats.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webgme/webgme-engine.git"
  },
  "keywords": [
    "webgme",
    "gme",
    "metamodeling",
    "cooperative",
    "modeling",
    "domain"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webgme/webgme-engine/issues"
  },
  "homepage": "https://github.com/webgme/webgme-engine#README.md",
  "dependencies": {
    "adm-zip": "0.4.7",
    "agentkeepalive": "3.3.0",
    "archiver": "2.1.1",
    "aws-sdk": "2.142.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.2",
    "browserify": "14.5.0",
    "buffer-equal-constant-time": "1.0.1",
    "chance": "1.0.12",
    "commander": "2.11.0",
    "compression": "1.7.1",
    "connect-multiparty": "2.1.0",
    "content-disposition": "0.5.2",
    "cookie-parser": "1.4.3",
    "debug": "2.6.8",
    "ejs": "2.5.7",
    "express": "4.16.2",
    "ink-docstrap": "1.3.0",
    "jsdoc": "3.5.5",
    "jsonwebtoken": "8.1.0",
    "marked": "0.3.12",
    "method-override": "2.3.10",
    "mime": "1.4.1",
    "minimatch": "3.0.4",
    "mongo-uri": "0.1.2",
    "mongodb": "2.2.33",
    "ncp": "2.0.0",
    "q": "1.5.1",
    "raml2html": "3.0.1",
    "redis": "2.8.0",
    "require-uncached": "1.0.3",
    "requirejs": "2.3.5",
    "socket.io": "2.0.4",
    "socket.io-client": "2.0.4",
    "socket.io-redis": "5.2.0",
    "superagent": "3.8.1",
    "text": "github:requirejs/text",
    "underscore": "1.8.3",
    "webgme-ot": "0.0.16",
    "webgme-webhook-manager": "0.1.0",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "esprima": "^4.0.0",
    "http-proxy": "^1.16.2",
    "istanbul": "^0.4.5",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-express-http-server": "^0.0.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-requirejs": "^1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^3.4.1",
    "msgpack-js": "^0.3.0",
    "pkginfo": "^0.4.1",
    "rimraf": "^2.6.2",
    "ssl-root-cas": "^1.2.4"
  }
}
